{
  "name": "solid-todo-prototype",
  "description": "Tasklist application implemented with Vue on top of Solid",
  "repository": "https://github.com/solidlabresearch/solid-todo-app-vue",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "css": "community-solid-server --config @css:config/default.json --seededPodConfigJson ./tests/users.json --loggingLevel error --port 3001",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue --ext .ts --ext .js --max-warnings 0",
    "prepare": "yarn lint && yarn build"
  },
  "config": {
    "community-solid-server": {
      "port": 3001,
      "loggingLevel": "error",
      "config": "@css:config/default.json",
      "seededPodConfigJson": "./tests/users.json"
    }
  },
  "dependencies": {
    "@comunica/query-sparql-link-traversal-solid": "^0.2.0",
    "@inrupt/solid-client-authn-browser": "^1.0.0",
    "typescript-cookie": "^1.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@solid/community-server": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vue-eslint-parser": "^9.0.0",
    "vue-tsc": "^1.0.0"
  }
}
