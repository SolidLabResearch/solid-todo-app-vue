{
  "name": "solid-todo-prototype",
  "description": "Tasklist application implemented with Vue on top of Solid",
  "repository": "https://github.com/solidlabresearch/solid-todo-app-vue",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "pod": "community-solid-server --rootFilePath ./data --config @css:config/file.json --loggingLevel error --port 3001",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue --ext .ts --ext .js --max-warnings 0"
  },
  "config": {
    "community-solid-server": {
      "port": 3001,
      "loggingLevel": "error",
      "config": "@css:config/file.json",
      "rootFilePath": "./data"
    }
  },
  "dependencies": {
    "@comunica/query-sparql-link-traversal-solid": "^0.0.2-alpha.6.0",
    "@inrupt/solid-client-authn-browser": "^1.12.4",
    "typescript-cookie": "^1.0.4",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@solid/community-server": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^30.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.9.0",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.11"
  }
}
